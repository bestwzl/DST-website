{
  "name": "gcp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "start:prod": "npm run serve -- --mode prod",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode dev",
    "build:pre": "vue-cli-service build --mode test",
    "build:prod": "vue-cli-service build  --mode prod",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "echarts": "^5.4.3",
    "element-ui": "^2.15.14",
    "less": "^4.2.0",
    "vue": "^2.6.14",
    "vue-router": "^3.1.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "axios": "^1.5.1",
    "babel-eslint": "^10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
